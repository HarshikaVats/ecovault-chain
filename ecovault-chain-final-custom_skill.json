  "servers": [
    {
      "url": "https://node-server-sainipratap-dev.apps.sandbox-m4.g2pi.p1.openshiftapps.com",
      "description": "Node server handling the calls"
    }
  ],
  "paths": {
    "/getBrands": {
      "get": {
        "summary": "Fetch the brands",
        "description": "Get the brands",
        "operationId": "getUser",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "bName": {
                        "type": "string"
                      },
                      "bStream": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/getOrders": {
      "post": {
        "summary": "Fetch orders by user credentials",
        "description": "Fetches the list of orders for a user based on email and password.",
        "operationId": "getOrders",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "description": "User's email"
                  },
                  "password": {
                    "type": "string",
                    "description": "User's password"
                  }
                },
                "required": [
                  "email",
                  "password"
                ]
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Success in fetching the brands",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "orders": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "orderID": {
                            "type": "string",
                            "example": "670bef7..."
                          },
                          "distance": {
                            "type": "string",
                            "example": "0.5 km"
                          },
                          "txn": {
                            "type": "string",
                            "example": "https://example.com"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/getProducts": {
      "post": {
        "summary": "Fetch products for a brand",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "brand": {
                    "type": "string",
                    "example": "nestle",
                    "description": "Name of the brand to fetch products"
                  }
                },
                "required": [
                  "brand"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "pName": {
                        "type": "string",
                        "example": "Maggi"
                      },
                      "pDesc": {
                        "type": "string",
                        "example": "2 min noodles"
                      },
                      "companyName": {
                        "type": "string",
                        "example": "nestle"
                      },
                      "pCategory": {
                        "type": "string",
                        "example": "food"
                      },
                      "pPrice": {
                        "type": "number",
                        "example": 8
                      },
                      "pLoc": {
                        "type": "string",
                        "example": "https://versatilevats.com/ennovation/glitch/allaboutclashing-Maggi.jpeg"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/getAOrder": {
      "post": {
        "summary": "Fetch an order by its ID",
        "description": "Get details of a specific order using orderID",
        "requestBody": {
          "description": "Order ID required to fetch the order details",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "orderID": {
                    "type": "string",
                    "example": "670bef71728920879205"
                  }
                },
                "required": [
                  "orderID"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Order found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "pickupAvailed": {
                      "type": "boolean"
                    },
                    "pickupLocation": {
                      "type": "string",
                      "example": "NA"
                    },
                    "time": {
                      "type": "string",
                      "example": "10/14/2024, 9:17:59 PM"
                    },
                    "totalPrice": {
                      "type": "integer",
                      "example": 330
                    },
                    "discount": {
                      "type": "string",
                      "example": "NA"
                    },
                    "carbonSaved": {
                      "type": "string",
                      "example": "NA"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}